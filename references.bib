pages = "2164--2173",
month =  may,
year =  1996,
url = "http://dx.doi.org/10.1128/MCB.16.5.2164",
file = "All Papers/M/Moore and Haber 1996 - Cell cycle and genetic requirements of two pathwa ... nd-joining repair of double-strand breaks in Saccharomyces cerevisiae.pdf",
language = "en",
issn = "0270-7306",
pmid = "8628283",
doi = "10.1128/MCB.16.5.2164",
