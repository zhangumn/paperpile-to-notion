keywords = "8-Oxo-G; Base Excision Repair; DNA Damage; E3 Ubiquitin Ligase; Mule; MutYH; Oxidative Stress; Ubiquitination",
language = "en",
issn = "0021-9258, 1083-351X",
pmid = "24443563",
doi = "10.1074/jbc.M113.536094",
