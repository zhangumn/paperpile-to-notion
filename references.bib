keywords = "CRISPR specificity; CRISPR-Cas9; DNA repair; protoplast; tomato",
language = "en",
issn = "2673-3439",
pmid = "37346168",
doi = "10.3389/fgeed.2023.1196763",
pmc = "PMC10279869"
}

