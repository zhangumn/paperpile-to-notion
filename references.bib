@ARTICLE{Belfield2018-qt,
title = "{DNA mismatch repair preferentially protects genes from mutation}",
author = "Belfield, Eric J and Ding, Zhong Jie and Jamieson, Fiona J C and Visscher, Anne M and Zheng, Shao Jian and Mithani, Aziz and Harberd, Nicholas P",
affiliation = "Department of Plant Sciences, University of Oxford, Oxford OX1 3RB, United Kingdom. State Key Laboratory of Plant Physiology and Biochemistry, College of Life Sciences, Zhejiang University, Hangzhou, 310058, China. Comparative Plant and Fungal Biology, Royal Botanic Gardens, Kew, Wakehurst Place, Ardingly, West Sussex RH17 6TN, United Kingdom. Department of Biology, Syed Babar Ali School of Science and Engineering, Lahore University of Management Sciences (LUMS), DHA, Lahore 54792, Pakistan.",
abstract = "Mutation is the source of genetic variation and fuels biological evolution. Many mutations first arise as DNA replication errors. These errors subsequently evade correction by cellular DNA repair, for example, by the well-known DNA mismatch repair (MMR) mechanism. Here, we determine the genome-wide effects of MMR on mutation. We first identify almost 9000 mutations accumulated over five generations in eight MMR-deficient mutation accumulation (MA) lines of the model plant species, Arabidopsis thaliana We then show that MMR deficiency greatly increases the frequency of both smaller-scale insertions and deletions (indels) and of single-nucleotide variant (SNV) mutations. Most indels involve A or T nucleotides and occur preferentially in homopolymeric (poly A or poly T) genomic stretches. In addition, we find that the likelihood of occurrence of indels in homopolymeric stretches is strongly related to stretch length, and that this relationship causes ultrahigh localized mutation rates in specific homopolymeric stretch regions. For SNVs, we show that MMR deficiency both increases their frequency and changes their molecular mutational spectrum, causing further enhancement of the GC to AT bias characteristic of organisms with normal MMR function. Our final genome-wide analyses show that MMR deficiency disproportionately increases the numbers of SNVs in genes, rather than in nongenic regions of the genome. This latter observation indicates that MMR preferentially protects genes from mutation and has important consequences for understanding the evolution of genomes during both natural selection and human tumor growth.",
journal = "Genome research",
volume =  28,
number =  1,
pages = "66--74",
month =  jan,
year =  2018,
url = "http://dx.doi.org/10.1101/gr.219303.116",
file = "All Papers/B/Belfield et al. 2018 - DNA mismatch repair preferentially protects genes from mutation.pdf",
language = "en",
issn = "1088-9051, 1549-5469",
pmid = "29233924",
doi = "10.1101/gr.219303.116",
pmc = "PMC5749183"
}

