file = "All Papers/M/Manova and Gruszka 2015 - DNA damage and repair in plants - from models to crops.pdf",
keywords = "Arabidopsis; DNA damage; DNA repair; crop plants; mutagenesis",
language = "en",
issn = "1664-462X",
pmid = "26557130",
doi = "10.3389/fpls.2015.00885",
pmc = "PMC4617055"
}

