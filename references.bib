pages = "1936--1941",
month =  "20~" # nov,
year =  2017,
url = "http://dx.doi.org/10.1021/acs.chemrestox.7b00152",
file = "All Papers/V/van Loon et al. 2017 - Living on the Edge - DNA Polymerase Lambda between Genome Stability and Mutagenesis.pdf",
language = "en",
issn = "0893-228X, 1520-5010",
pmid = "28841305",
doi = "10.1021/acs.chemrestox.7b00152"
}

