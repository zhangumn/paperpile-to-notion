@ARTICLE{Dixon2016-lc,
title = "{NanoLuc Complementation Reporter Optimized for Accurate Measurement of Protein Interactions in Cells}",
author = "Dixon, Andrew S and Schwinn, Marie K and Hall, Mary P and Zimmerman, Kris and Otto, Paul and Lubben, Thomas H and Butler, Braeden L and Binkowski, Brock F and Machleidt, Thomas and Kirkland, Thomas A and Wood, Monika G and Eggers, Christopher T and Encell, Lance P and Wood, Keith V",
