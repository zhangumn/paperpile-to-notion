@ARTICLE{Shimizu2008-gq,
title = "{Selectable tolerance to herbicides by mutated acetolactate synthase genes integrated into the chloroplast genome of tobacco}",
author = "Shimizu, Masanori and Goto, Maki and Hanai, Moeko and Shimizu, Tsutomu and Izawa, Norihiko and Kanamoto, Hirosuke and Tomizawa, Ken-Ichi and Yokota, Akiho and Kobayashi, Hirokazu",
